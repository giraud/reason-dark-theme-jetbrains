<application>
    <component name="DefaultFont">
        <option name="FONT_SIZE" value="13" />
        <option name="FONT_FAMILY" value="PragmataPro" />
        <option name="LINE_SPACING" value="1.1" />
        <option name="USE_LIGATURES" value="true" />
    </component>
    <component name="TodoConfiguration">
        <pattern pattern="\btodo\b.*" />
        <pattern pattern="\bfixme\b.*" />
        <pattern icon="important" pattern="\bzzz\b.*" />
        <filter name="zzz">
            <pattern index="2" />
        </filter>
    </component>
</application>